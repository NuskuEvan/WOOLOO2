<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WOOLOOGENERATION">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="224dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/FANTASY"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="243dp"
        android:layout_height="137dp"
        app:srcCompat="@drawable/CHAOS"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="192dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="265dp"
        android:layout_height="157dp"
        app:srcCompat="@drawable/MEDIEVAL"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="379dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="239dp"
        android:layout_height="169dp"
        app:srcCompat="@drawable/HELL"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="562dp" />

    <Button
        android:id="@+id/es_fantasy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FANTASY"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="43dp" />

    <Button
        android:id="@+id/es_chaos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/black"
        android:text="APOCOLYPSE"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="249dp" />

    <Button
        android:id="@+id/es_medieval"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MEDIEVAL"
        tools:layout_editor_absoluteX="286dp"
        tools:layout_editor_absoluteY="426dp" />

    <Button
        android:id="@+id/es_hell"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#830018"
        android:text="HELL"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="625dp" />

</androidx.constraintlayout.widget.ConstraintLayout>